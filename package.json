{
  "name": "example-nodejs-transformer",
  "version": "2.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"npm run watch-compile\" \"npm run watch-dev\"",
    "watch-compile": "swc src -w --out-dir dist --copy-files",
    "watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/main.js",
    "build:watch": "yarn build && concurrently \"npm run watch-build\" \"npm run watch-package\"",
    "watch-build": "swc src -w --out-dir dist --copy-files",
    "watch-package": "chokidar \"package.json\" -c \"copyfiles package.json dist\"",
    "build": "swc src -d dist && copyfiles package.json dist",
    "start": "NODE_ENV=production node dist/main.js",
    "clean": "rm -rf ./dist",
    "lint": "eslint --ext .ts src --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "prepare": "husky install",
    "reconfigure": "commitizen init cz-conventional-changelog --yarn --dev --exact --force"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.80",
    "@swc/jest": "^0.2.27",
    "@types/express": "^4.17.17",
    "@types/jest": "29.5.3",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.7",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "axios": "^1.5.0",
    "chokidar": "^3.5.3",
    "chokidar-cli": "^3.0.0",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.1",
    "copyfiles": "^2.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "dayjs": "^1.11.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.2",
    "jest-junit": "16.0.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "symlink-dir": "^5.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.2.2",
    "wait-for-expect": "^3.0.2",
    "zod": "^3.22.2"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
